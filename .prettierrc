{
  "semi": false,
  "trailingComma": "none",
  "tabWidth": 2,
  "arrowParens": "always",
  "printWidth": 100,
  "singleQuote": true,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "importOrder": [
    "^@nestjs/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "decorators-legacy"
  ]
}